<%= link_to "", patient_path, :class => "link_onb" %>
<%= link_to "", main_user_path, :class => "link_main" %>
<%= link_to "", history_user_path, :class => "link_history" %>
<%= link_to "", advs_user_path, :class => "link_advs" %>

<script type="text/javascript">
  $(document).ready(function(){

    $(".home").click(function(){
      $(".link_main").get(0).click();
    });

    $(".ads").click(function(){
      $(".link_advs").get(0).click();
    });
    $(".onb").click(function(){
      $(".link_onb").get(0).click();
    });

    $(".history").click(function(){
      $(".link_history").get(0).click();
    });

    $(".my").click(function(){
      $(".toggle").addClass('left');
      $(".wall").click(function(){
        $(".toggle").removeClass('left');
      })
    })
  });
</script>
